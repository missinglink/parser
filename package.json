{
  "name": "pelias-parser",
  "version": "0.0.0-development",
  "author": "pelias",
  "description": "Pelias parsing library",
  "homepage": "https://github.com/pelias/parser",
  "license": "MIT",
  "main": "server/http.js",
  "scripts": {
    "units": "./bin/test unit",
    "funcs": "./bin/test func",
    "server": "node ./server/http.js",
    "test": "./bin/test",
    "ci": "./bin/test",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts}\" --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts}\" --ignore-path .gitignore",
    "validate": "npm ls",
    "prune": "npm prune"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pelias/parser.git"
  },
  "keywords": [
    "pelias",
    "parser"
  ],
  "bugs": {
    "url": "https://github.com/pelias/parser/issues"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "cluster": "^0.7.7",
    "express": "^4.16.4",
    "pluralize": "^8.0.0",
    "remove-accents": "^0.4.2",
    "stringbuffer": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "better-sqlite3": "^7.4.1",
    "chalk": "^2.4.2",
    "csv-parse": "^4.4.1",
    "deep-eql": "^4.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.1.3",
    "precommit-hook": "^3.0.0",
    "prettier": "^2.4.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.4.0"
  },
  "pre-commit": [
    "lint",
    "prune",
    "validate",
    "test"
  ],
  "release": {
    "branch": "master",
    "success": []
  }
}
